{
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "g",
                "e",
                "s"
            ],
            "commands": [
                "workbench.action.files.save"
            ],
            "silent": true
        },
        {
            "before": [
                "\\",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ],
            "silent": true
        },
        {
            "before": [
                "]",
                "d"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ],
            "silent": true
        },
        {
            "before": [
                "[",
                "d"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ],
            "silent": true
        },
    ],
    // Colors:
    "editor.tokenColorCustomizations": {
        "[DuoTone Dark Sea]": {
            "textMateRules": [
                {
                    "scope": "punctuation",
                    "settings": {
                        "foreground": "#a3b4c4"
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Gruvbox Light Soft]": {
            "titleBar.activeBackground": "#F2E5BC",
            "titleBar.activeForeground": "#63584f"
        },
        "[DuoTone Dark Sea]": {
            "activityBar.background": "#1D262F",
            "statusBar.background": "#273c53",
            "statusBar.noFolderBackground": "#273c53",
            "sideBar.background": "#1D262F",
            "titleBar.activeBackground": "#1D262F",
            "tab.activeBackground": "#232D38",
            "tab.inactiveBackground": "#1D262F",
            "editorGroupHeader.tabsBackground": "#1D262F",
            "input.background": "#232D38"
        },
        "[Gruvbox Dark (Hard)]": {
            "statusBar.background": "#3C6C58",
            "statusBar.noFolderBackground": "#528464",
            "statusBar.debuggingBackground": "#D49729",
            "activityBar.background": "#1D2021",
            "titleBar.activeBackground": "#1D2021",
            "tab.activeBackground": "#304041",
            "tab.inactiveBackground": "#1D2021",
            "editorGroupHeader.tabsBackground": "#1D2021"
        },
        "[Darktooth]": {
            "statusBar.background": "#4D8080",
            "statusBar.noFolderBackground": "#528464",
            "statusBar.debuggingBackground": "#C66644"
        },
        "[Gruvbox Material Dark]": {
            "statusBar.foreground": "#d6c6b3",
            "statusBar.background": "#3C6C58",
            "statusBar.noFolderBackground": "#528464",
            "statusBar.debuggingBackground": "#D49729",
            "activityBar.background": "#1D2021",
            "titleBar.activeBackground": "#1D2021",
            "tab.activeBackground": "#304041",
            "tab.inactiveBackground": "#1D2021",
            "editorGroupHeader.tabsBackground": "#1D2021"
        },
        "sideBar.border": "#4a855f",
        "panel.border": "#c06881",
        "terminal.border": "#c38f2e",
        "editorGroup.border": "#4a835e",
        "peekView.border": "#c38f2e",
        "statusBar.background": "#2b7275",
        "statusBar.noFolderBackground": "#2b7275",
        "statusBar.debuggingBackground": "#2b7275",
        "statusBar.foreground": "#ffffff",
        "statusBar.debuggingForeground": "#ffffff"
    },
    "files.autoSave": "onFocusChange",
    "workbench.startupEditor": "newUntitledFile",
    "python.linting.pylintArgs": [
        "--extension-pkg-whitelist=numpy,wx"
    ],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.editor.tabCloseButton": "left",
    "workbench.sideBar.location": "right",
    "[javascriptreact]": {},
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "todohighlight.defaultStyle": {
        "color": "#000000",
        "backgroundColor": "#fff",
    },
    "todohighlight.isEnable": false,
    "vim.disableExtension": false,
    "[python]": {
        "editor.tabSize": 4,
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
    },
    "editor.renderWhitespace": "none",
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "python.linting.enabled": true,
    "pasteImage.path": "${currentFileDir}/.images",
    "editor.tabCompletion": "on",
    "gitlens.views.repositories.files.layout": "tree",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "breadcrumbs.enabled": true,
    "editor.suggestSelection": "first",
    "latex-workshop.view.pdf.viewer": "tab",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.rulers": [
        80,
        120
    ],
    "vim.showMarksInGutter": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.insert": "#ab5781",
    "vim.statusBarColors.normal": "#2b7275",
    "vim.statusBarColors.visual": "#6a8a50",
    "vim.statusBarColors.replace": "#c25960",
    "python.defaultInterpreterPath": "/usr/local/bin/python3",
    "python.languageServer": "Default",
    "notebook.output.textLineLimit": 1000,
    "editor.folding": true,
    "cSpell.userWords": [
        "appendleft",
        "Assistantship",
        "denoms",
        "diagonalization",
        "dups",
        "extendleft",
        "fontawesome",
        "fortawesome",
        "freqs",
        "Glines",
        "Hola",
        "issuetracker",
        "llms",
        "logn",
        "mindmap",
        "mlogm",
        "nlogn",
        "nums",
        "openai",
        "Pomodoro",
        "quadtree",
        "reactflow",
        "soln",
        "thoughtorganizer"
    ],
    "boot-java.rewrite.reconcile": true,
    "javascript.inlayHints.parameterTypes.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "all",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "cmake.configureOnOpen": true,
    "editor.inlineSuggest.enabled": true,
    "github.copilot.advanced": {},
    "editor.formatOnSave": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
    },
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "terminal.external.osxExec": "iTerm2.app",
    "editor.minimap.enabled": false,
    "vim.history": 200,
    "editor.smoothScrolling": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.neovimConfigPath": "~/.config/nvim/init.vim",
    "search.quickOpen.includeSymbols": true,
    "redhat.telemetry.enabled": true,
    "editor.linkedEditing": true,
    "git.autofetch": true,
    "vim.neovimUseConfigFile": true,
    "vim.enableNeovim": true,
    "latex-workshop.latexindent.path": "/Library/TeX/texbin/latexindent",
    "latex-workshop.latex.recipe.default": "lastUsed",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.maxTokenizationLineLength": 2500,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.tabSize": 2,
    "cSpell.ignoreWords": [
        "nodrag"
    ],
    "window.zoomLevel": 1,
    "workbench.editor.tabActionLocation": "left",
    "git.openRepositoryInParentFolders": "never",
    "security.promptForLocalFileProtocolHandling": false,
    "lldb.library": "/Applications/Xcode-beta.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "lldb.launch.expressions": "native",
    "lldb.suppressUpdateNotifications": true,
    "update.showReleaseNotes": false,
    "editor.lineNumbers": "relative",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "javascript": false
    },
}